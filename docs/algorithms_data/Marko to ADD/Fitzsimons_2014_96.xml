<algorithm_description>

  <algorithm_type>transport</algorithm_type>
  <position_in_process>transport</position_in_process>
  <material>stone</material>
  <source_type>experimental archaeology</source_type>
  <source_chronology>modern</source_chronology>
  <source_geography>Egypt</source_geography>

</algorithm_description>



<algdata>

  <algorithm_statement>Transporting a 7 ton stone statue by 130 men through a distance of 1 km in 17 days</algorithm_statement>
  <algorithm_math>Number of p-d = (kg -weight- / 1000) * (m -distance- / 3,1578)</algorithm_math>
  <variables>
    <var1>
      <value></value><description>weight</description>
      <unit>kg</unit>
      <op></op>
    </var1>
    <var2>
      <value></value><description>distance</description>
      <unit>m</unit>
      <op></op>
    </var2>
    <var3>
      <value></value><description></description>
      <unit></unit>
      <op></op>
    </var3>
  </variables>

<!-- comment  and so on for as many variables as needed, up to 9, always seperated by an _op_  -->

  <results>
    <result1>
      <value></value><description></description>
      <op>(var1 / 1000) * (var2 / 3,1578)</op>
      <unit>p-d</unit>
    </result1>
    <result2>
      <value></value><description></description>
      <op></op>
      <unit></unit>
    </result2>
  </results>
  
<!-- comment   and so on for as many results as needed, up to 9 -->

</algdata>



<algnotes>

  <general_notes>The author does not mention the number of hours worked in one person-day</general_notes>
