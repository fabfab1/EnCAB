<algdata>

  <algorithm_statement>Excavating earth</algorithm_statement>
  <algorithm_math>1 man excavated 2.6 cubic meters in 5 hours</algorithm_math>
  <variables>
    <var1>
      <unit>m3</unit><description></description>
      <op>var2 * 2.6 / 5</op>
    </var1>
    <var2>
      <unit>p-h</unit><description></description>
      <op>var1 / 2.6 * 5</op>
    </var2>
  </variables>

</algdata>



