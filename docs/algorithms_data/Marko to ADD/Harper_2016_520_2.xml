<algdata>

  <algorithm_statement>limestone was quarried in one person-hour</algorithm_statement>
  <algorithm_math>0,089 m3 quarried in 1 p-h</algorithm_math>
  <variables>
    <var1>
      <value>1</value><description></description>
      <unit>p-h</unit>
      <op>var2 / 0,089</op>
    </var1>
    <var2>
      <value>0,089</value><description></description>
      <unit>m3</unit>
      <op>var1 * 0,089</op>
    </var2>
  </variables>

