<algdata>

  <algorithm_statement>1 p-h  to cut out sod in m3 </algorithm_statement>
  <algorithm_math>1,254 m3 of sod in 1 p-h</algorithm_math>
  <variables>
    <var1>
      <value>1</value><description></description>
      <unit>p-h</unit>
      <op>var2 / 1,254</op>
    </var1>
    <var2>
      <value>1,254</value><description></description>
      <unit>m3</unit>
      <op>var1 * 1,254</op>
    </var2>
  </variables>

<!-- comment  and so on for as many variables as needed, up to 9, always seperated by an _op_  -->

  <results>
    <result1>
