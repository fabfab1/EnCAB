      <value>1</value><description></description>
      <unit>p-h</unit>
      <op>var2 / 0,137</op>
    </var1>
    <var2>
      <value>0,137</value><description></description>
      <unit>m3</unit>
      <op>var1 * 0,137</op>
    </var2>
  </variables>

