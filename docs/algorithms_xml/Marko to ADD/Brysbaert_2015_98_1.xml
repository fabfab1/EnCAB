<algdata>

  <algorithm_statement>Loading earth into baskets in p-h</algorithm_statement>
  <algorithm_math>1 m3 of earth in 0,6 p-h</algorithm_math>
  <variables>
    <var1>
      <unit>m3</unit><description></description>
      <op>var2 * 1</op>
    </var1>
    <var2>
      <unit>p-h</unit><description></description>
      <op>var1 / 1</op>
    </var2>
  </variables>

</algdata>



