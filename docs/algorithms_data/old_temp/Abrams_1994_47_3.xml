<algorithm_description>

  <algorithm_type>transport</algorithm_type>

  <position_in_process>transport</position_in_process>
  <material>earth</material>
  <source_type></source_type>
  <source_chronology></source_chronology>
  <source_geography></source_geography>

</algorithm_description>


<algdata>

  <algorithm_statement>Transportation</algorithm_statement>
  <algorithm_math>Simplified formula based on ENCAFE_1957_22. Output = Q x (L/v+ L/V') x H.</algorithm_math>
  <variables>
    <var1>
      <unit>kg</unit><description>Q</description>
    </var1>
    <var2>
      <unit>m</unit><description>L</description>
    </var2>
    <var3>
      <unit>mxm</unit><description>V</description>
    </var3>
    <var4>
      <unit>mxm</unit><description>V'</description>
    </var4>
    <var5>
      <unit>hr</unit><description>H</description>
    </var5>
  </variables>
  <results>
    <result1>
      <unit>m3</unit><description>Output</description>
      <op>var1 * (var2 / var3 + var2 / var4) * var5</op>
    </result1>
  </results>
</algdata>


<algnotes>

  <general_notes>Output = Q x (L/v+ L/V') x H. Q=quantity of earth per load (in kg), L=transport distance(in m), V=velocity loaded (given here in meters per minute), V'=velocity unloaded (given here in meters per minute), H=hours, Output expressed in m3.</general_notes>
  <biblio_notes></biblio_notes>

</algnotes>


