<algdata>

  <algorithm_statement>1 p-h excavating average 1,64 m3 of soil</algorithm_statement>
  <algorithm_math>1 p-h dig 1,64 m3</algorithm_math>
  <variables>
    <var1>
      <value>1</value><description></description>
      <unit>p-h</unit>
      <op>var2 / 1,64</op>
    </var1>
    <var2>
      <value>1,64</value><description></description>
      <unit>m3</unit>
      <op>var1 * 1,64</op>
    </var2>
  </variables>

<!-- comment  and so on for as many variables as needed, up to 9, always seperated by an _op_  -->

  <results>
    <result1>
      <value></value><description></description>
      <op></op>
      <unit></unit>
    </result1>
    <result2>
      <value></value><description></description>
      <op></op>
      <unit></unit>
    </result2>
  </results>
  
<!-- comment   and so on for as many results as needed, up to 9 -->

</algdata>



