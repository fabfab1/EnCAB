  <algorithm_math>1 man excavated 2.6 cubic meters in 5 hours</algorithm_math>
  <variables>
    <var1>
      <unit>m3</unit><description></description>
      <op>var2 * 2.6 / 5</op>
    </var1>
    <var2>
