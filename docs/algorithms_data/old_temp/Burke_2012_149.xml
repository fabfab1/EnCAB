<algdata>

  <algorithm_statement>One labourer dag 0,3 m3 of soil in one hour</algorithm_statement>
  <algorithm_math>0,3 m3 dag in 1 p-h</algorithm_math>
  <variables>
    <var1>
      <value>1</value><description></description>
      <unit>p-h</unit>
      <op>var2 / 0,3</op>
    </var1>
    <var2>
      <value>0,3</value><description></description>
      <unit>m3</unit>
      <op>var1 * 0,3</op>
    </var2>
  </variables>

<!-- comment  and so on for as many variables as needed, up to 9, always seperated by an _op_  -->

  <results>
    <result1>
      <value></value><description></description>
      <op></op>
      <unit></unit>
    </result1>
    <result2>
      <value></value><description></description>
      <op></op>
      <unit></unit>
    </result2>
  </results>
  
<!-- comment   and so on for as many results as needed, up to 9 -->

</algdata>



