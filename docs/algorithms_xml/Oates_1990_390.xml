<algdata>

  <algorithm_statement>100 bricks require 1 1/2 sacks of straw, approx 60kg.</algorithm_statement>
  <algorithm_math>1 brick requires 0.6kg straw.</algorithm_math>
  <variables>
    <var1>
      <unit>brick</unit><description>number of mudbricks</description>
      <op>var2 / 0.6 </op>
    </var1>
    <var2>
      <unit>kg</unit><description>Kilos of straw</description>
      <op>var1 * 0.6</op>
    </var2>
  </variables>

</algdata>


<algnotes>

  <general_notes>The brick size is unspecified.</general_notes>
