<algdata>

  <algorithm_statement>Building an ashlar stone wall in one person-hour</algorithm_statement>
  <algorithm_math>0,019 m3 built in 1 p-h</algorithm_math>
  <variables>
    <var1>
      <unit>p-h</unit><description></description>
      <op>var2 / 0,019</op>
    </var1>
    <var2>
      <unit>m3</unit><description></description>
      <op>var1 * 0,019</op>
    </var2>
  </variables>

</algdata>



