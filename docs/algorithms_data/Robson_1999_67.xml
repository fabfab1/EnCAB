<algdata>

  <algorithm_statement>1/6 of the volume of a mudbrick wall consists of mortar</algorithm_statement>
  <algorithm_math>m3 * 5/6 = volume of mudbricks</algorithm_math>
  <equation_direction></equation_direction>
  <variables>
    <var1>
      <unit>m3</unit><description>wall volume</description>
      <op>var2 * 6/5</op>
    </var1>
    <var2>
      <unit>m3</unit><description>mortar</description>
      <op>var1 * 1/6</op>
    </var2>
  </variables>


  <results>
    <result1>
      <unit>m3</unit><description>mudbricks in wall volume</description>
      <op>var1 * (5/6)</op>
    </result1>
  </results>

</algdata>



