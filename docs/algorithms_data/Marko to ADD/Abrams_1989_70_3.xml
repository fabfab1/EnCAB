  <algorithm_math>1 person collected 3000 litres in one hour</algorithm_math>
  <variables>
    <var1>
      <unit>hr</unit><description></description>
      <op>var2 / 3000</op>
    </var1>
    <var2>
      <unit>l</unit><description></description>
      <op>var1 * 3000</op>
    </var2>
  </variables>

</algdata>



