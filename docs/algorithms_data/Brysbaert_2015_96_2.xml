<algdata>

  <algorithm_statement>Loading large limestone blocks in 20 minutes by 8 men</algorithm_statement>
  <algorithm_math>Loading and unloading of maximum 2,5 - 3,5 tonne blocks can be done in 2,66666 p-h</algorithm_math>
  <variables>
    <var1>
      <unit>p-h</unit><description></description>
      <op>var2 / 2,66666</op>
    </var1>
    <var2>
      <unit>kg</unit><description></description>
      <op>var1 * 2,66666</op>
    </var2>
  </variables>

</algdata>



