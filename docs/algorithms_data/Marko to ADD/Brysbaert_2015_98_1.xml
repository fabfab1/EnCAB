<algdata>

  <algorithm_statement>Loading earth into baskets in p-h</algorithm_statement>
  <algorithm_math>1 m3 of earth in 0,6 p-h</algorithm_math>
  <variables>
    <var1>
      <value>1</value><description></description>
      <unit>m3</unit>
      <op>var2 * 1</op>
    </var1>
    <var2>
      <value>0,6</value><description></description>
      <unit>p-h</unit>
      <op>var1 / 1</op>
    </var2>
  </variables>

<!-- comment  and so on for as many variables as needed, up to 9, always seperated by an _op_  -->

  <results>
    <result1>
      <value></value><description></description>
      <op></op>
      <unit></unit>
    </result1>
    <result2>
      <value></value><description></description>
      <op></op>
      <unit></unit>
    </result2>
  </results>
  
<!-- comment   and so on for as many results as needed, up to 9 -->

</algdata>



