<algdata>

  <algorithm_statement>Applying clay plaster to floors in one person hour</algorithm_statement>
  <algorithm_math>5,69 m2 dressed in 1 p-h</algorithm_math>
  <variables>
    <var1>
      <value>1</value><description></description>
      <unit>p-h</unit>
      <op>var2 / 5,69</op>
    </var1>
    <var2>
      <value>5,69</value><description></description>
      <unit>m2</unit>
      <op>var1 * 5,69</op>
    </var2>
  </variables>

