<algorithm_description>

  <algorithm_type>transport</algorithm_type>

  <position_in_process>transport</position_in_process>
  <material>earth</material>
  <source_type></source_type>
  <source_chronology></source_chronology>
  <source_geography></source_geography>

</algorithm_description>



<algdata>

  <algorithm_statement>Transportation</algorithm_statement>
  <algorithm_math>Simplified formula based on ENCAFE_1957_22. Output = Q x (L/v+ L/V') x H.</algorithm_math>
  <variables>
    <var1>
        <unit></unit><description>Q</description>
    </var1>
    <var2>
      <unit></unit><description>L</description>
    </var2>
    <var3>
      <unit></unit><description>v</description>
    </var3>
    <var4>
      <unit></unit><description>V'</description>
    </var4>
    <var5>
      <unit></unit><description>H</description>
    </var5>
  </variables>
  <results>
    <result1>
      <unit></unit><description></description>
      <op>var1 * (var2 / var3 + var2 / var4) * var5</op>
    </result1>
</results>
</algdata>



<algnotes>

  <general_notes>Output = Q x (L/v+ L/V') x H. Q=quantity of earth per load (in kg), L=transport distance(in m), V=velocity loaded, V'=velocity unloaded, H=hours, Output expressed in m3.</general_notes>
  <biblio_notes></biblio_notes>

</algnotes>



