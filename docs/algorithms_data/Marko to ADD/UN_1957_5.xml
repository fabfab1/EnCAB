<algdata>

  <algorithm_statement>0,42 m3 dry hard clay can be excavated per person per hour</algorithm_statement>
  <algorithm_math>0,42 m3 dry hard clay in 1 p-h</algorithm_math>
  <variables>
    <var1>
      <value>1</value><description></description>
      <unit>p-h</unit>
      <op>var2 / 0,42</op>
    </var1>
    <var2>
      <value>0,42</value><description></description>
      <unit>m3</unit>
      <op>var1 * 0,42</op>
    </var2>
  </variables>

<!-- comment  and so on for as many variables as needed, up to 9, always seperated by an _op_  -->

