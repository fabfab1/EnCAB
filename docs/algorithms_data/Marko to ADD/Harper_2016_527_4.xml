<algdata>

  <algorithm_statement>Building roof in one person-hour</algorithm_statement>
  <algorithm_math>0,4 m3 built in 1 p-h</algorithm_math>
  <variables>
    <var1>
      <value>1</value><description></description>
      <unit>p-h</unit>
      <op>var2 / 0,4</op>
    </var1>
    <var2>
      <value>0,4</value><description></description>
      <unit>m3</unit>
      <op>var1 * 0,4</op>
    </var2>
  </variables>

<!-- comment  and so on for as many variables as needed, up to 9, always seperated by an _op_  -->

  <results>
    <result1>
      <value></value><description></description>
